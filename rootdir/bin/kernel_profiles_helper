#!/system/bin/sh

# cpu profiles
chown system:system /sys/devices/system/cpu/cpufreq/policy0/schedutil/up_rate_limit_us
chown system:system /sys/devices/system/cpu/cpufreq/policy0/schedutil/down_rate_limit_us
chown system:system /sys/devices/system/cpu/cpufreq/policy0/schedutil/hispeed_freq
chown system:system /sys/devices/system/cpu/cpufreq/policy0/scaling_min_freq
chown system:system /sys/devices/system/cpu/cpufreq/policy0/scaling_max_freq
chown system:system /sys/devices/system/cpu/cpufreq/policy0/schedutil/hispeed_load
chown system:system /sys/devices/system/cpu/cpufreq/policy0/scaling_governor

chown system:system /sys/devices/system/cpu/cpufreq/policy6/schedutil/up_rate_limit_us
chown system:system /sys/devices/system/cpu/cpufreq/policy6/schedutil/down_rate_limit_us
chown system:system /sys/devices/system/cpu/cpufreq/policy6/schedutil/hispeed_freq
chown system:system /sys/devices/system/cpu/cpufreq/policy6/scaling_min_freq
chown system:system /sys/devices/system/cpu/cpufreq/policy6/scaling_max_freq
chown system:system /sys/devices/system/cpu/cpufreq/policy6/schedutil/hispeed_load
chown system:system /sys/devices/system/cpu/cpufreq/policy6/scaling_governor

chown system:system /sys/class/kgsl/kgsl-3d0/min_clock_mhz
chown system:system /sys/class/kgsl/kgsl-3d0/max_clock_mhz
chown system:system /sys/class/kgsl/kgsl-3d0/devfreq/governor

chown system:system /sys/block/sda/queue/scheduler
chown system:system /sys/block/sda/queue/read_ahead_kb
 
chown system:system /sys/block/sde/queue/scheduler
chown system:system /sys/block/sde/queue/read_ahead_kb

